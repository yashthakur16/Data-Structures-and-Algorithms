#include<iostream>
using namespace std;

int main()
{
    int arr[5]={64,25,12,22,11};
    int min=INT16_MAX;
    for(int i=0;i<5;i++)
    {
        for(int j=i+1;j<5;j++)
        {
            if(arr[j]<arr[i])
            {
                swap(arr[i],arr[j]);
            }
        }
        
    }
    for(int i=0;i<5;i++)
    {
        cout<<arr[i]<<" ";
    }

    return 0;

}
